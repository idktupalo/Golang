package main

import "fmt"

type animal struct {
	voice string
}

type Raccon struct {
	animal
	friendly bool
}

type Otter struct {
	animal
	waterFriendly bool
}

func show(i ...interface{}) (res []interface{}) {
	for _, val := range i {
		res = append(res, val)
	}
	return
}

func main() {
	raccon := Raccon{animal{"kaba4"}, true}
	otter := Otter{animal{"kawai"}, true}
	fmt.Println(show(raccon, otter))
}
